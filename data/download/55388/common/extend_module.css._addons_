/** forum::viewthread **/
.pi { overflow: hidden;margin-bottom: 10px;height: 16px;border-bottom: 0 none;}
.pi strong a {float: right;margin-top: -6px;padding: 4px 6px;border: 0 none;font-weight: 400;}
.pls { width: 170px;background: none repeat scroll 0% 0% #F3F3F3;overflow: hidden;border-right: 0 none;}
h1.ts { font-size:18px;}
.h_pls.ptn { padding-top:10px !important;}
.h_plc.pbn { padding-bottom:10px !important;}
.h_plc .y {padding-top:5px;}
.pgbtn a { display: block;margin-bottom: 10px;border: 0 none;border-radius: none;text-align: center;}
