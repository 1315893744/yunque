/** forum **/
.sxp_mn .bm_h { height: 42px;line-height: 42px;border:0 none;border-image: none;font-weight: bold;}
.fl .bm_h { padding-left:15px;background:#F0F0F0;}
.fl { border:0 none;background: none repeat scroll 0% 0% transparent; }
.fl .bm { margin-bottom: 15px;background:#f8f8f8;}
.bmw .bm_h a { color:#333;font-weight:normal;}
.bmw .bm_h a:hover { color:#2A2A2A !important;}
span.y { color:#333;font-weight:normal; }
.bm_h h2 a { float: none;margin: 0px;font-size: 16px;font-weight: normal;}
.fl_g dt a,.fl_tb h2 a { font-size: 16px; color:#444;}
.xw0.xi1 { font-size:16px;}
.bm_h .o img { margin-top:12px;}
.fl_tb td { padding: 20px;}
.fl_row td { border: medium none;}
#pt .y {width: auto;height: 29px; overflow: hidden;}
#chart {margin-bottom:15px; }
.chart { height: 70px;padding: 15px 0px;border: 5px solid #F5F5F5;background: none repeat scroll 0% 0% transparent;color: #434A53;overflow: hidden;}
.chart ul {width: 950px;height: 100%;overflow:hidden;}
.chart li {float: left;width: 237px;height: 100%;line-height: 29px;border-right: 1px solid #E7E7E7;text-align: center;font-size:16px; }
.chart em {display: block;line-height: 37px;color: #434A53;font-size: 26px;}
#chartfei { width:236px;}
#chartby { border-right:0 none;}

.bm { border: 0px none;border-radius: 0px 0px 0px 0px;}
.lk { background:#F8F8F8;padding-bottom:10px;margin-bottom:5px;}
#online { background:#F8F8F8;margin-bottom:15px;}
#online .bm_h { background:#F8F8F8;}

.h_bm_c tr { height: 40px;}
.tl th em, .tl th em a {color: {MENUBGCOLOR};font-size:15px;}
.tl .th { margin: 0 10px;padding: 0px 10px;border-bottom: 0 none;background: none repeat scroll 0% 0% #F2F2F2;}
.h_ttp {margin-bottom: 0;padding: 20px 0 10px 0;background:#FFF !important;}
.h_ttp li { padding:0 10px 0 0;margin-left:10px;border-right:1px solid #D3D3E0;}
.h_ttp li a { font-size: 14px; color: #666;border:0 none;padding:0;}
.h_ttp .a a { padding-right: 12px;padding:0;border-color: #D3D3E0;background: none repeat scroll 0% 0% #FFF;color:{MENUBGCOLOR};}
.h_ttp a:hover { border-color:#D3D3E0;color:{MENUBGCOLOR};text-decoration: none;}
.h_ttp .a a:hover { background: none;}
.tl .h_ts th, .tl .h_ts td { padding:0px;border:0 none;background: none repeat scroll 0% 0% #F2F2F2;}
.bbs { border-bottom: 0 none !important;}
#postsubmit.pn { color:#FFF;font-weight:normal;}

/*next page*/
#autopbn { display: block;margin-bottom: 10px;border:0 none;border-radius: none;text-align: center;}
#autopbn.bm_h { height:40px;line-height:40px;}
/** end **/

/** forum::forumdisplay **/
.sxp_mn .bm_h h2 { color:#FFF;font-size:14px;}
.sxp_mn .fl { background:#FFF;}
.fl_g dt a, .fl_tb h2 a {font-size: 14px;color: #444;}
#f_pst .bm_h { padding: 0px 10px;height: 40px;border: 0 none;background: none repeat scroll 0% 0% #F8F8F8;line-height: 40px;
white-space: nowrap;overflow: hidden;}
